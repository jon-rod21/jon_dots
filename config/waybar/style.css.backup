* {
	border: none;
	border-radius: 10;
    font-family: "JetBrainsMono Nerd Font", "JetBrains Mono", monospace;
	font-size: 15px;
	font-weight: bold;
	min-height: 10px;
}

window#waybar {
	background: transparent;
}

#network {
    margin-top: 10px;
    margin-left: 8px;
    padding-left: 10px;
    padding-right: 10px;
    margin-bottom: 0px;
    border-radius: 10px;
    transition: none;
    background: #1e1e2e;
    color: #cba6f7;
    border-bottom: 3px;
    border-style: solid;
    border-color: #1e1e2e;
    transition: 0.3s;
}

#network:hover {
    color: #1e1e2e;
    background: #cba6f7;
}

