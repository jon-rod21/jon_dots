#window {
  background-color: #1e1e2e;
  font-family: "JetBrainsMono Nerd Font";
  font-size: 15px;
  font-weight: bold;
  padding: 0 10px;
  border-radius: 10px;
  margin: 10px 8px 0px 8px;
  border-bottom: 3px solid #1e1e2e;
  min-width: 200px;
  transition: 0.3s;
}

#window:hover {
  background: #cba6f7;
  color: #1e1e2e;
}


#custom-bluetooth {
  background-color: #1e1e2e;
